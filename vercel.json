{
    "version": 2,
    "builds": [
        {
            "src": "/src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.js"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "missing": [
                {
                    "type": "cookie",
                    "key": "accessToken"
                }
            ],
            "destination": "/login"
        }
    ]
}